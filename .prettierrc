{
    "endOfLine": "lf",
    "semi": false,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "es5",
    "printWidth": 120,
    "importOrder": [
      "^(react/(.*)$)|^(react$)",
      "^(next/(.*)$)|^(next$)",
      "<THIRD_PARTY_MODULES>",
      "",
      "^~/pages/(.*)$",
      "",
      "^types$",
      "^~/types/(.*)$",
      "^~/config/(.*)$",
      "^~/lib/(.*)$",
      "^~/hooks/(.*)$",
      "^~/components/ui/(.*)$",
      "^~/components/(.*)$",
      "^~/styles/(.*)$",
      "",
      "^[./]"
    ],
    "plugins": [
      "@ianvs/prettier-plugin-sort-imports",
      "prettier-plugin-tailwindcss"
    ]
  }